{
  "coc.preferences.formatOnSaveFiletypes": [
    "python"
  ],
  "python.formatting.provider": "yapf",
  "python.jediEnabled": false,
  "languageserver": {
    "lua": {
      "command": "/home/glesaaen/.config/coc/extensions/coc-lua-data/sumneko-lua-ls/bin/Linux/lua-language-server",
      "args": [
        "-E",
        "/home/glesaaen/.config/coc/extensions/coc-lua-data/sumneko-lua-ls/main.lua"
      ],
      "filetypes": [
        "lua"
      ],
      "rootPatterns": [
        ".git/"
      ],
      "settings": {
        "Lua": {
          "workspace": {
            "library": {
              "/usr/share/nvim/runtime/lua": true,
              "/usr/share/nvim/runtime/lua/vim/lsp": true
            },
            "maxPreload": 2000,
            "preloadFileSize": 1000
          },
          "runtime": {
            "version": "5.4.1"
          },
          "diagnostics": {
            "enable": true,
            "globals": [
              "hs",
              "vim",
              "it",
              "describe",
              "before_each",
              "after_each"
            ],
            "disable": [
              "lowercase-global"
            ]
          }
        }
      }
    }
  }
}
